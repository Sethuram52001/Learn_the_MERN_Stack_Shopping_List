{
  "name": "mern_shopping_list",
  "version": "1.0.0",
  "description": "Shopping list build with the MERNstack - sethu",
  "main": "server.js",
  "scripts": {
    "client-install" : "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev" : "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client"
  },
  "author": "Sethuram",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "engines": {
        "node": "14.13.1",
        "npm": "6.14.8"
  }
}
